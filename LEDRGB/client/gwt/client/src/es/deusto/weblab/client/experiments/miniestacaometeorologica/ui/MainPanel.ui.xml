<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:es.deusto.weblab.client.ui.widgets">

	<ui:with field="i18n" type="es.deusto.weblab.client.i18n.IWebLabI18N" />

	
	<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_CENTER" borderWidth="0" spacing="10" height="300" styleName="" ui:field="widget">
		<g:VerticalPanel ui:field="cam">
			<g:Grid height="867px">
				<g:row>
					<g:customCell>
						<p1:WlWebcam height="243px" ui:field="camera"/>
					</g:customCell>
					<g:customCell>
						<p1:WlWebcam ui:field="GraficoHumidade"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<p1:WlWebcam ui:field="GraficoTemperatura"/>
					</g:customCell>
					<g:customCell>
						<p1:WlWebcam ui:field="GraficoCO2"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<p1:WlWebcam ui:field="GraficoLuz"/>
					</g:customCell>
					<g:customCell>
						<p1:WlWebcam ui:field="GraficoSom"/>
					</g:customCell>
				</g:row>
			</g:Grid>
				<p1:WlTimer ui:field="timer" horizontalAlignment="ALIGN_CENTER"/>
			</g:VerticalPanel>
	 </g:VerticalPanel>
	
</ui:UiBinder> 